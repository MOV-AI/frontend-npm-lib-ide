{
  "name": "@mov-ai/mov-fe-lib-ide",
  "version": "1.4.0-0",
  "keywords": [
    "frontend",
    "ide",
    "react",
    "lib"
  ],
  "homepage": "https://github.com/MOV-AI/frontend-npm-lib-ide/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/MOV-AI/frontend-npm-lib-ide.git"
  },
  "license": "ISC",
  "author": "MOV.AI",
  "sideEffects": true,
  "exports": {
    ".": {
      "import": "./esm/mov-ide.js",
      "types": "./types/mov-ide.d.ts",
      "default": "./cjs/mov-ide.js"
    }
  },
  "main": "cjs/mov-ide.js",
  "module": "esm/mov-ide.js",
  "browser": "web/mov-ide.js",
  "types": "types/mov-ide.d.ts",
  "files": [
    "cjs",
    "web",
    "esm",
    "types"
  ],
  "scripts": {
    "build": "scripts types && scripts build",
    "build-storybook": "build-storybook",
    "buildDev": "webpack --mode=development --watch",
    "clean": "rm -rf cjs web esm types",
    "fast": "scripts build",
    "lock": "mv ../../pnpm-workspace.yaml ../../pnpm-workspace.yaml.bak ; pnpm i ; mv ../../pnpm-workspace.yaml.bak ../../pnpm-workspace.yaml",
    "prepare": "husky",
    "storybook": "start-storybook -p 6006",
    "test": "scripts test",
    "testDev": "jest --watchAll"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@remixproject/engine": "^0.3.19",
    "@remixproject/engine-web": "^0.3.19",
    "d3": "^5.16.0",
    "joi": "^17.12.3",
    "monet": "^0.9.2",
    "prop-types": "^15.8.1",
    "rc-dock": "^3.2.19",
    "react-virtualized": "^9.22.3",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-virtualized-tree": "^3.4.1",
    "react-window": "^1.8.7",
    "rxjs": "^7.8.1",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@codingame/monaco-languageclient": "^0.17.4",
    "@mov-ai/mov-fe-lib-code-editor": "workspace:^1.1.4-0",
    "@mov-ai/mov-fe-lib-core": "workspace:^1.2.3-0",
    "@mov-ai/mov-fe-lib-react": "workspace:^1.3.7-0",
    "@mov-ai/mov-fe-lib-scripts": "^0.6.8-1",
    "@mov-ai/mov-fe-lib-sub": "^2.3.2-1",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.55",
    "http-proxy-middleware": "^3.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "lodash": "^4.17.21",
    "prettier": "3.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sort-package-json": "2.10.1"
  },
  "peerDependencies": {
    "@mov-ai/mov-fe-lib-code-editor": "workspace:^1.1.4-0",
    "@mov-ai/mov-fe-lib-core": "workspace:^1.2.3-0",
    "@mov-ai/mov-fe-lib-react": "workspace:^1.3.7-0",
    "@mov-ai/mov-fe-lib-sub": "^2.3.2-1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "packageManager": "pnpm@9.11.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/mov-ai"
  },
  "entry": "./index.js",
  "external": {
    "lodash": "global _",
    "react": "global React",
    "@mov-ai/mov-fe-lib-core": "global Movai",
    "@mov-ai/mov-fe-lib-react": "global MovaiReact",
    "@mov-ai/mov-fe-lib-code-editor": "global MovaiCode",
    "@mov-ai/mov-fe-lib-sub": "global ttySub"
  },
  "library": "MovaiIDE",
  "resolve": {
    "vscode": "@codingame/monaco-languageclient/lib/vscode-compatibility"
  }
}
